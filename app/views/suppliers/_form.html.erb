<%= form_with model: @supplier do |form| %>
  <div class="form-group">
    <%= form.label :name %><br>
    <%= form.text_field :name, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :mobileno %><br>
    <%= form.text_field :mobileno, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :city %><br>
    <%= form.text_field :city, class:"form-control" %>
  </div>

  <div>
    <%= form.submit class:"btn btn-secondary",data: {confirm: "Are you sure?"} %>
  </div>
<% end %>