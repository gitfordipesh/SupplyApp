<h3 class="text-center">All Accounts</h3>
<br>
<br>
<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Account No.</th>
        <th scope="col">Name</th>
        <th scope="col">Supplier Id.</th>
      </tr>
    </thead>
    <% @accounts.each do |a| %>
    <tbody>
      <tr>
        <td><%= a.id %></td>
        <td><%= a.accNo %></td>     
        <td><%= a.supplier.name %></td>     
        <td><%= a.supplier_id %></td>     
    </tr>
    </tbody>
  <% end %>
</table>