<h1>Create New Account</h1>

<div class = "jumbotron">
<%= form_with(model: @account, url: supplier_accounts_path) do |form| %>
<div class="form-group">
  <p>
    <%= form.label :Account_no %><br>
    <%= form.text_field :accNo , class:"form-control" %>
  </p>  

  <p>
    <%= form.submit class:"btn btn-secondary"%>
  </p>
</div>
<% end %>
</div>
